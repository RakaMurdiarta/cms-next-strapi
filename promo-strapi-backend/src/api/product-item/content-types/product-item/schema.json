{
  "kind": "collectionType",
  "collectionName": "product_items",
  "info": {
    "singularName": "product-item",
    "pluralName": "product-items",
    "displayName": "Product_Item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "content": {
      "type": "blocks"
    },
    "thumbnail": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "subTitle": {
      "type": "string"
    },
    "price": {
      "type": "biginteger"
    },
    "discount": {
      "type": "integer"
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "product_item"
    }
  }
}
